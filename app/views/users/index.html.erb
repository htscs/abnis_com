<h1>All users</h1>
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user, :size => 30 %>
      <%= link_to user.name, user_path(user.id)%>
    </li>
  <% end %>
</ul>
<%= will_paginate @users, :prev_label => '<<', :next_label => '>>', :inner_window => 0, :outer_window => 0 %>
<%= page_entries_info @users %>